{
    "name": "kitchen-sink-angular",
    "version": "1.0.0",
    "description": "App for testing community components in Angular",
    "private": true,
    "main": "index.js",
    "scripts": {
        "dev": "npm run clean && nx run spa-angular:map-node-modules && npm run copy:icons && npx ng serve",
        "build-app": "npm run clean && nx run spa-angular:map-node-modules && npm run copy:icons && npx ng build",
        "clean": "rm -rf node_modules .angular",
        "preview": "nx run spa-angular:preview",
        "jest:test": "nx run spa-angular:map-node-modules && jest",
        "e2e:headless:ci": "CI=true start-server-and-test preview http://localhost:4200 'cypress run --record'",
        "e2e:headless": "CI=true start-server-and-test dev http://localhost:4200 'cypress run'",
        "e2e:interactive": "CI=true start-server-and-test dev http://localhost:4200 'cypress open'",
        "copy:icons": "copyfiles --up 1 node_modules/@maersk-global/icons/js/**/* public/assets/"
    },
    "license": "MIT",
    "dependencies": {
        "@maersk-global/fonts": "^1.6.0",
        "@maersk-global/icons": "^1.23.0"
    }
}