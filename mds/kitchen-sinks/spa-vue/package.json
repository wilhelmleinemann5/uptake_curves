{
    "name": "kitchen-sink-vue",
    "version": "1.0.0",
    "description": "App for testing community components in VUE",
    "private": true,
    "main": "index.js",
    "scripts": {
        "dev": "npm run clean && nx run spa-vue:map-node-modules && vite",
        "build-app": "npm run clean && nx run spa-vue:map-node-modules && vue-tsc --noEmit && vite build",
        "clean": "rm -rf node_modules .vite",
        "preview": "vite preview",
        "e2e:headless:ci": "start-server-and-test preview http://localhost:4400 'cypress run --record'",
        "e2e:headless": "start-server-and-test dev http://localhost:4400 'cypress run'",
        "e2e:interactive": "start-server-and-test dev http://localhost:4400 'cypress open'"
    },
    "license": "MIT",
    "dependencies": {
        "@maersk-global/fonts": "^1.6.0",
        "@maersk-global/icons": "^1.23.0",
        "vue-router": "^4.4.0",
        "vuex": "^4.1.0",
        "vue": "^3.4.21"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0.4",
        "rollup-plugin-copy": "^3.5.0",
        "typescript": "^5.2.2",
        "vite": "^6.3.5",
        "vue-tsc": "^2.0.7"
    }
}