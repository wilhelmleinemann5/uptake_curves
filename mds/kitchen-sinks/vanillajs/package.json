{
  "name": "vanillajs",
  "version": "1.0.0",
  "description": "App for testing community components in VanillaJs",
  "type": "commonjs",
  "main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "private": true,
  "scripts": {
    "dev": "npm run clean && nx run vanillajs:map-node-modules && vite",
    "build-app": "npm run clean && nx run vanillajs:map-node-modules && tsc && vite build",
    "clean": "rm -rf node_modules .vite",
    "preview": "vite preview",
    "e2e:headless:ci": "start-server-and-test preview http://localhost:4500 'cypress run --record'",
    "e2e:headless": "start-server-and-test dev http://localhost:4500 'cypress run'",
    "e2e:interactive": "start-server-and-test dev http://localhost:4500 'cypress open'"
  },
  "license": "MIT",
  "dependencies": {
    "@maersk-global/fonts": "^1.6.0",
    "@maersk-global/icons": "^1.23.0"
  },
  "devDependencies": {
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.2.2",
    "tslib": "^2.3.0",
    "vite": "^6.3.5"
  }
}