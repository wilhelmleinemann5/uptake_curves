{
  "name": "mds",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "targets": {
    "serve": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "./.storybook"
      }
    },
    "serve:vr": {
      "executor": "nx:run-script",
      "options": {
        "script": "serve:vr"
      }
    },
    "serve:accessibility": {
      "executor": "nx:run-script",
      "options": {
        "script": "serve:accessibility"
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": [
        "{options.outputDir}"
      ],
      "options": {
        "outputDir": "dist/storybook/mds",
        "configDir": "./.storybook"
      }
    },
    "ui:build": {
      "executor": "nx:run-script",
      "options": {
        "script": "ui:build"
      }
    },
    "ui:build:ci": {
      "executor": "nx:run-script",
      "options": {
        "script": "ui:build:ci"
      }
    },
    "ui:build:vr": {
      "executor": "nx:run-script",
      "options": {
        "script": "ui:build:vr"
      }
    },
    "ui:build:accessibility": {
      "executor": "nx:run-script",
      "options": {
        "script": "ui:build:accessibility"
      }
    },
    "preview-storybook": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "mds:build-storybook",
        "staticFilePath": "./dist/storybook/mds",
        "port": 5000
      }
    },
    "component-test:interactive": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "cypress.config.ts",
        "testingType": "component",
        "devServerTarget": "mds:serve",
        "port": 4400,
        "watch": true
      }
    },
    "component-test:headless": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "cypress.config.ts",
        "testingType": "component",
        "skipServe": true
      }
    },
    "component-test:headless:ci": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "record": false
      }
    },
    "add-new-component": {
      "executor": "nx:run-script",
      "options": {
        "script": "add-new-component"
      }
    },
    "accessibility:headless:ci": {
      "executor": "nx:run-script",
      "options": {
        "script": "accessibility:headless:ci"
      }
    },
    "accessibility:headless": {
      "executor": "nx:run-script",
      "options": {
        "script": "accessibility:headless"
      }
    },
    "accessibility:interactive": {
      "executor": "nx:run-script",
      "options": {
        "script": "accessibility:interactive"
      }
    },
    "chromatic": {
      "executor": "nx:run-script",
      "options": {
        "script": "chromatic"
      }
    },
    "chromatic:list": {
      "executor": "nx:run-script",
      "options": {
        "script": "chromatic:list"
      }
    },
    "lighthouse:run": {
      "executor": "nx:run-script",
      "options": {
        "script": "lighthouse:run"
      }
    },
    "lighthouse:prod": {
      "executor": "nx:run-script",
      "options": {
        "script": "lighthouse:prod"
      }
    },
    "lighthouse:build:prod": {
      "executor": "nx:run-script",
      "options": {
        "script": "lighthouse:build:prod"
      }
    },
    "test:ssr:map-node-modules": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "recursive-copy -w dist/packages dist/node_modules/@maersk-global -f '**/*.*'"
        ]
      }
    },
    "test:ssr": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "web-test-runner packages/**/tests/**/*.ssr.js"
        ]
      },
      "dependsOn": [
        "test:ssr:map-node-modules"
      ]
    },
    "metadata": {
      "executor": "nx:run-script",
      "options": {
        "script": "metadata"
      }
    },
    "generate-metadata:cache:ux-docs": {
      "executor": "nx:run-script",
      "options": {
        "script": "generate-metadata:cache:ux-docs"
      }
    },
    "generate-metadata:check-files": {
      "executor": "nx:run-script",
      "options": {
        "script": "generate-metadata:check-files"
      }
    },
    "extract-metadata": {
      "executor": "nx:run-script",
      "options": {
        "script": "extract-metadata"
      }
    },
    "serve:mcp-inspector": {
      "executor": "nx:run-script",
      "options": {
        "script": "serve:mcp-inspector"
      }
    }
  }
}