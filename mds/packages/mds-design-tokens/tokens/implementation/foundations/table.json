{
    "foundations": {
        "table": {
            "border-width": {
                "value": "$global.border.width"
            },
            "border-radius": {
                "value": "$brand.border.large.radius"
            },
            "border-style": {
                "value": "$global.border.style"
            },
            "border-color": {
                "value": "$brand.appearance.neutral.default.border-color"
            },
            "row-selector": {
                "margin-top": {
                    "value": -3
                }
            },
            "row-expander": {
                "margin-top": {
                    "value": -3
                }
            },
            "row-expander-trigger": {
                "transition-duration": {
                    "value": "350ms"
                }
            },
            "cell": {
                "typography": {
                    "value": "text-medium_normal"
                },
                "background-color": {
                    "value": "$brand.appearance.neutral.default.background-color"
                },
                "text-color": {
                    "value": "$brand.appearance.neutral.default.on-background-color"
                },
                "hover": {
                    "background-color": {
                        "value": "$brand.appearance.state.neutral.default.hover.background-color"
                    },
                    "text-color": {
                        "value": "$brand.appearance.neutral.default.on-background-color"
                    }
                },
                "selected": {
                    "background-color": {
                        "value": "$brand.appearance.primary.weak.background-color"
                    },
                    "text-color": {
                        "value": "$brand.appearance.primary.weak.on-background-color"
                    }
                }
            },
            "header-cell": {
                "typography": {
                    "value": "text-medium_bold"
                },
                "sort-button": {
                    "font-weight": {
                        "value": "$brand.typography.text.medium_bold.font-weight"
                    }
                },
                "background-color": {
                    "value": "$brand.appearance.neutral.weakest.background-color"
                },
                "text-color": {
                    "value": "$brand.appearance.neutral.weakest.on-background-color"
                },
                "sort": {
                    "hover": {
                        "background-color": {
                            "value": "$brand.appearance.state.neutral.default.hover.background-color"
                        }
                    },
                    "active": {
                        "background-color": {
                            "value": "$brand.appearance.state.neutral.default.active.background-color"
                        }
                    }
                }
            },
            "footer-cell": {
                "typography": {
                    "value": "text-medium_bold"
                }
            },
            "zebra-stripe": {
                "background-color": {
                    "value": "$brand.appearance.neutral.weakest.background-color"
                },
                "text-color": {
                    "value": "$brand.appearance.neutral.weakest.on-background-color"
                }
            },
            "caption": {
                "text-color": {
                    "value": "$brand.appearance.neutral.default.on-background-color"
                }
            },
            "subtext": {
                "text-color": {
                    "value": "$brand.appearance.neutral.weak.text-color"
                },
                "font-weight": {
                    "value": "$brand.typography.text.medium_normal.font-weight"
                }
            },
            "small": {
                "caption": {
                    "typography": {
                        "value": "text-small_normal"
                    },
                    "margin-bottom": {
                        "value": 12
                    }
                },
                "cell": {
                    "typography": {
                        "value": "text-small_normal"
                    },
                    "padding-top": {
                        "value": 6
                    },
                    "padding-right": {
                        "value": 12
                    },
                    "padding-bottom": {
                        "value": 6
                    },
                    "padding-left": {
                        "value": 12
                    }
                },
                "header-cell": {
                    "typography": {
                        "value": "text-small_bold"
                    },
                    "sortable": {
                        "padding-top": {
                            "value": 5
                        },
                        "padding-bottom": {
                            "value": 5
                        }
                    }
                },
                "subtext": {
                    "typography": {
                        "value": "text-x-small_normal"
                    }
                },
                "expanded-row": {
                    "padding-top": {
                        "value": 12
                    },
                    "padding-right": {
                        "value": 12
                    },
                    "padding-bottom": {
                        "value": 20
                    },
                    "padding-left": {
                        "value": 12
                    }
                }
            },
            "medium": {
                "caption": {
                    "typography": {
                        "value": "text-medium_normal"
                    },
                    "margin-bottom": {
                        "value": 16
                    }
                },
                "cell": {
                    "typography": {
                        "value": "text-medium_normal"
                    },
                    "padding-top": {
                        "value": 8
                    },
                    "padding-right": {
                        "value": 16
                    },
                    "padding-bottom": {
                        "value": 8
                    },
                    "padding-left": {
                        "value": 16
                    }
                },
                "header-cell": {
                    "typography": {
                        "value": "text-medium_bold"
                    },
                    "sortable": {
                        "padding-top": {
                            "value": 7
                        },
                        "padding-bottom": {
                            "value": 7
                        }
                    }
                },
                "subtext": {
                    "typography": {
                        "value": "text-small_normal"
                    }
                },
                "expanded-row": {
                    "padding-top": {
                        "value": 16
                    },
                    "padding-right": {
                        "value": 16
                    },
                    "padding-bottom": {
                        "value": 24
                    },
                    "padding-left": {
                        "value": 16
                    }
                }
            },
            "large": {
                "caption": {
                    "typography": {
                        "value": "text-medium_normal"
                    },
                    "margin-bottom": {
                        "value": 24
                    }
                },
                "cell": {
                    "typography": {
                        "value": "text-medium_normal"
                    },
                    "padding-top": {
                        "value": 12
                    },
                    "padding-right": {
                        "value": 24
                    },
                    "padding-bottom": {
                        "value": 12
                    },
                    "padding-left": {
                        "value": 24
                    }
                },
                "header-cell": {
                    "typography": {
                        "value": "text-medium_bold"
                    },
                    "sortable": {
                        "padding-top": {
                            "value": 11
                        },
                        "padding-bottom": {
                            "value": 11
                        }
                    }
                },
                "subtext": {
                    "typography": {
                        "value": "text-small_normal"
                    }
                },
                "expanded-row": {
                    "padding-top": {
                        "value": 24
                    },
                    "padding-right": {
                        "value": 24
                    },
                    "padding-bottom": {
                        "value": 32
                    },
                    "padding-left": {
                        "value": 24
                    }
                }
            }
        }
    }
}