{
  "name": "mds-design-tokens",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/mds-design-tokens",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "dependsOn": [
        "build:tokens",
        "copy:assets",
      ]
    },
    "build:tokens": {
      "command": "node packages/mds-design-tokens/build"
    },
    "copy:assets": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "cp packages/mds-design-tokens/package.json dist/packages/mds-design-tokens/package.json"
        ]
      },
      "dependsOn": [
        "build:tokens",
        "copy:assets"
      ]
    },
    "stats": {
      "command": "node packages/mds-design-tokens/build/stats"
    },
    "generate:metadata": {
      "command": "node tools/scripts/generate-metadata/design-tokens/generate.mjs"
    },
    "release": {
      "command": "node tools/scripts/publish.mjs mds-design-tokens",
      "options": {
        "outputPath": "dist/packages/mds-design-tokens"
      }
    }
  },
  "tags": []
}