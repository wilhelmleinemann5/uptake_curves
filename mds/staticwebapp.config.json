{
    "routes": [
        {
            "route": "/?path=/story/welcome--page",
            "redirect": "/?path=/story/welcome--welcome",
            "statusCode": 301
        },
        {
            "route": "/*",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ],
    "responseOverrides": {
        "401": {
            "statusCode": 302,
            "redirect": "/.auth/login/aad?post_login_redirect_uri=.referrer"
        }
    },
    "auth": {
        "identityProviders": {
            "azureActiveDirectory": {
                "registration": {
                    "openIdIssuer": "https://login.microsoftonline.com/05d75c05-fa1a-42e7-9cf1-eb416c396f2d/v2.0",
                    "clientIdSettingName": "AZURE_CLIENT_ID",
                    "clientSecretSettingName": "AZURE_CLIENT_SECRET"
                }
            }
        }
    }
}