{
  "name": "mds",
  "version": "2.135.5",
  "license": "UNLICENSED",
  "description": "Maersk Design System - framework agnostic web components",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Maersk-Global/mds.git"
  },
  "scripts": {
    "postinstall": "chmod ug+x .husky/* && git config --local commit.template 'gitmessage.txt'",
    "prepare": "husky install",
    "lint": "nx run-many --target=lint --tuiAutoExit",
    "build": "nx run-many --target=build --tuiAutoExit",
    "metadata": "nx run-many --target=generate:metadata --tuiAutoExit",
    "serve": "nx run mds:serve",
    "serve:vr": "STORYBOOK_MODE=vr npm run serve",
    "serve:accessibility": "STORYBOOK_MODE=accessibility npm run serve",
    "serve:accessibility:ci": "npm run ui:build:accessibility && cd dist/storybook/mds && http-server -p 4400",
    "preview": "nx run mds:preview-storybook",
    "build-storybook": "nx run mds:build-storybook",
    "ui:build": "nx run mds:build-storybook",
    "ui:build:feat": "nx run mds:build-storybook --configuration=ci",
    "ui:build:ci": "nx run mds:build-storybook --configuration=ci && npm run copy:static-web-app-config",
    "ui:build:vr": "STORYBOOK_MODE=vr nx run mds:build-storybook --configuration=ci",
    "ui:build:accessibility": "STORYBOOK_MODE=accessibility nx run mds:build-storybook --configuration=ci",
    "clean-ignored": "rm -rf dist node_modules .nx",
    "copy:static-web-app-config": "recursive-copy ./staticwebapp.config.json ./dist/storybook/mds/staticwebapp.config.json",
    "component-test:interactive": "nx run mds:component-test:interactive",
    "component-test:headless": "nx run mds:component-test:headless",
    "component-test:headless:ci": "nx run mds:component-test:headless:ci",
    "add-new-component": "rimraf -rf ./node_modules/.cache/storybook && node ./tools/scripts/add-new-component/index.mjs",
    "accessibility:headless:ci": "start-server-and-test serve:accessibility:ci http://localhost:4400 'cypress run'",
    "accessibility:headless": "start-server-and-test serve:accessibility:ci http://localhost:4400 'cypress run'",
    "accessibility:interactive": "start-server-and-test serve:accessibility http://localhost:4400 'cypress open'",
    "test:ssr": "npx nx run mds:test:ssr",
    "chromatic": "npx chromatic --build-script-name ui:build:vr",
    "chromatic:list": "npx chromatic --build-script-name ui:build:vr --list",
    "lighthouse:run": "rimraf -rf ./dist/lighthouse && rimraf -rf ./.lighthouseci && lhci autorun --emptyOutDir --config=./lighthouse/.lighthouserc.cjs",
    "lighthouse:prod": "npm run lighthouse:build:prod && vite preview --config=./lighthouse/vite.config.mjs",
    "lighthouse:build:prod": "vite build --config=./lighthouse/vite.config.mjs",
    "generate:medalia": "node tools/scripts/medalia/generate.mjs",
    "generate-metadata:cache:ux-docs": "node tools/scripts/generate-metadata/populate-ux-docs-cache.mjs",
    "generate-metadata:check-files": "node tools/scripts/generate-metadata/check-missing-metadata.mjs",
    "extract-metadata": "node tools/scripts/generate-metadata/extract-metadata.mjs",
    "serve:mcp-inspector": "nx run mds-mcp-server:build && CLIENT_PORT=8080 SERVER_PORT=9000 MCP_CONNECTION=dev npx @modelcontextprotocol/inspector node dist/packages/mds-mcp-server/index.js"
  },
